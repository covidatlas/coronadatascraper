           // if ($(this).val() == "Yes") {
                $("#dvT").hide();
				$("#dvC").show();
				$("#dvH").hide();
				$("#dvS").hide();
				$("#dvE").hide();
				$("#dvA").hide();
				$("#cases").hide();
				$("#latest").hide();
